<div class="container">
    <div class="text-center"><h3>Registered Stores</h3></div>
    <br>
    <table class="table table-striped">
        <input type="hidden" id="_csrf" value="{{csrfToken}}">
        <thead>
            <tr>
            <th>Date</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            </tr>
        </thead>
        <tbody>
            {{#each stores}}
                <tr>
                    <td>{{moment created_at}}</td>
                    <td>{{first_name}}</td>
                    <td>{{last_name}}</td>
                    <td>{{email}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>